fun main(){
    val person1 = Employee(name: "Asan", surName: "Usenov", age: 27, experience: 2)
    val person2 = Employee(name: "Esen", surName: "Kadyrov", age: 30, experience: 10)
    val person3 :Employee = person1.copy()


    println(person1.toString())
    println(person1.hashCode())
    println(person1==person2)

}

data class Employee(
        val name: String,
        val surName: String,
        val age: Int,
        val experience: Int){

}

